		"@unocss/extractor-svelte": "66.1.2",
		"@unocss/preset-icons": "^66.1.2",
		"@unocss/preset-rem-to-px": "^66.1.2",
		"@unocss/reset": "^66.1.2",
		"clsx": "^2.1.1",
		"discord-api-types": "0.38.8",
		"discord-markdown-parser": "^1.2.0",
		"dotenv": "^16.5.0",
